package dsa_programs_array;

public class MinimumElement {

	public static void main(String[] args) {
         int arr[]= {5,2,3,1,0};
         int min=arr[0];
         for(int i=0;i<arr.length-1;i++)
         {
        	 if(arr[i+1]<arr[i])
        	 {
        		  min=arr[i+1];
        	 }
         }
         
         System.out.println(min);
         
         
	}

}
