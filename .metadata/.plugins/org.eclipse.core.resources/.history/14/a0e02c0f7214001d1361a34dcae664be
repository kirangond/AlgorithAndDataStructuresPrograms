package dsa_programs_array;

public class MaximumSubSumArray {

	public static void main(String[] args) {
       int arr[]= {-5,4,6,-3,4,-1};
       int max_sum=0;
       for(int i=0;i<arr.length;i++)
       {
    	   int sum=0;
    	  for(int j=i;j<arr.length;j++)
    	  {
    		  sum=sum+arr[j];
    		 
    		  
    	  }
    	  if(sum>max_sum)
    	  {
    		  max_sum=sum;
    	  }
    	  
       }
       System.out.println(max_sum);
	}

}
