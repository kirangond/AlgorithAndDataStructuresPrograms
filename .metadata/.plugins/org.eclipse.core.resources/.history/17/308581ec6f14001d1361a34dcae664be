package dsa_programs_array;

public class MaximumSubSumArray {

	public static void main(String[] args) {
       int arr[]= {1,5,3,2,1};
       int sum=0;
       for(int i=0;i<arr.length;i++)
       {
    	  for(int j=i;j<arr.length;j++)
    	  {
    		  sum=sum+arr[j];
    		 
    		  
    	  }
    	  System.out.println(sum);
       }
	}

}
