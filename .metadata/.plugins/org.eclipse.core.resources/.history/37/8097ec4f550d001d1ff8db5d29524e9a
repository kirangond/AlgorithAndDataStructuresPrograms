package algorithm.search;

public class LinearSearchFunction {

	public static void main(String[] args) {

		 int arr[]= {2,4,5,3,1};
		 int key=5;
		 
		 linearSearch(arr,key);
		
	}

	private static int linearSearch(int[] arr, int key) {
		    for(int i=0;i<arr.length-1;i++)
		    {
		    	if(arr[i]==key)
		    	{
		    		return 1;
		    	}
		    }
		    return -1;
	}

}
