package com.kadens;

public class MaximumSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int arr[] = {4,6,-3,4};
		int size = 4;
		
		int max = 0;
		
		for (int i = 0;i<4;i++)
		{
			int sum = 0;
			for(int j = i; j<4;j++)
			{
				sum = sum + arr[j];
			}
			
			if (sum>max)
			{
				max = sum;
			}
		}
		
		System.out.println("max: "+ max);

	}

}
