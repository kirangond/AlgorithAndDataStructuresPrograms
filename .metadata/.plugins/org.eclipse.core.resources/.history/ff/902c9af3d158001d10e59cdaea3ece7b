package array_log2base2;

public class LargestElement {

	public static void main(String[] args) {
		int arr[]= {8, 10, 3, 2, 6};
		 int maximum_max=0;
		    for(int i=0;i<=arr.length;i++)
		    {
		         int minimum_max=0;
		        if(arr[i]>arr[i+1])
		          minimum_max=arr[i];
		          if(minimum_max>maximum_max)
		           maximum_max=minimum_max;
		    }
		    System.out.println("Max ="+maximum_max);

	}

}
